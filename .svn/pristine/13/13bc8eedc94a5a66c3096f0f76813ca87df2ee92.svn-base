*{margin: 0px;
	padding: 0px;}
.workarea {
	width: 100%;
	display: flex;
	flex-direction: row;
}
.viewarea {
	flex: 0 0 350px; 
	display: flex;
	flex-direction: column;
}
.viewarea .view-panel {
	width: 100%;
	height: 100%;
	background-repeat: no-repeat;
	background-size: cover;
	color: #ffffff;
}
.viewarea .view-panel.lb {
	background-image: url(../../images/itembox.png);
	background-position: left top;
}
.viewarea .view-panel.b {
	background-image: url(../../images/a-r-box.png);
	background-position: left top;
}
.viewarea .view-panel.rb {
	background-image: url(../../images/a-zx-box.png);
	background-position: left top;
}
.viewarea .view-panel  .label {
	color: #089CA9;
	font-size: 14px;/*需要根据分辨率调*/
	font-weight: bold;
	line-height:26px;
	padding-left: 15px;
}

.viewarea .view-panel.airDyn{
	flex: 1;
	display:flex;
	flex-direction: column
}
.viewarea .view-panel.airBroadcast{
	flex: 2;display:flex;flex-direction: column
}
.viewarea .view-panel.airChange {
	flex: 1;
	display:flex;
	flex-direction:column;
}

.viewarea .view-panel.contrast {
	flex: 1;
	display:flex;
	flex-direction:column;
}
.viewarea .view-panel .contrastEchart{
	flex:1;
	border: 1px solid transparent;
}
.remind {
	font-size: 12px;
	text-align: right;
	margin-right: 30px;
	height: 25px;
}

.viewarea .view-panel .basic-info {
	display: flex;
	padding-left: 10px;
}

.viewarea .view-panel .basic-info p {
	flex: 1;
	line-height:30px;
	color: #000000;
	text-align: center;
	font-weight: bold;
	background-repeat: no-repeat;
	background-size:cover;
}
.viewarea .view-panel .basic-info p.basic-one {
	background-image: url(../../images/bg-item1.png);
	background-position:center;
	margin-right: 40px;
}
.viewarea .view-panel .basic-info p.basic-two {
	background-image: url(../../images/bg-item2.png);
	background-position:center;
	margin-right: 10px;
}
.viewarea .view-panel .basic-info p.basic-three {
	background-image: url(../../images/bg-item3.png);
	background-position:center;
	margin-right: 40px;
}
.viewarea .view-panel .basic-info p.basic-four{
	background-image: url(../../images/bg-item4.png);
	background-position:center;
	margin-right: 10px;
}
.viewarea .view-panel .detail-info {
	margin:0px 5px 0px 5px;
	background: url("../../images/m-l-i-infobox.png") no-repeat left top;
	background-size: 100% 100%;
	flex: 2;
	display: flex;
	flex-direction: column;
}

.viewarea .view-panel .detail-info .de-info-num{
	flex:2;
	display: flex;
	flex-direction: row;
}
.viewarea .view-panel .detail-info .de-info-num p{
	flex:1;
	text-align: center;
	line-height: 60px;
}
.viewarea .view-panel .detail-info .de-info-item p.number{
	line-height: 60px;
	text-align: center;
}
.viewarea .view-panel .detail-info .de-info-num .num-bg-one {
	background: url("../../images/bg-de-info1.png") no-repeat center;
	background-size: cover;
	color: #F0A936;
}
.viewarea .view-panel .detail-info .de-info-num .num-bg-two {
	background: url("../../images/bg-de-info2.png") no-repeat center;
	background-size: cover;
	color: #22C260;
}
.viewarea .view-panel .detail-info .de-info-num .num-bg-three {
	background: url("../../images/bg-de-info3.png") no-repeat center;
	background-size: cover;
	color: #26A1DA;
}
.viewarea .view-panel .detail-info .de-info-num .num-bg-four {
	background: url("../../images/bg-de-info4.png") no-repeat center;
	background-size: cover;
	color: #FC3838;
}
.viewarea .view-panel .detail-info .de-info-num .num-bg-five {
	background: url("../../images/bg-de-info5.png") no-repeat center;
	background-size:cover;
	color: #CF4FC7;
}
.viewarea .view-panel .detail-info .de-info-num .num-bg-six {
	background: url("../../images/bg-de-info6.png") no-repeat center;
	background-size: cover;
	color: #01D3BC;
}

.viewarea .view-panel .detail-info .de-info-txt{
	flex:1;
	display: flex;
	flex-direction:row;
}
.viewarea .view-panel .detail-info .de-info-txt .txt{
	flex:1;
	height:20px;
	color: #000000;
	font-size: 14px;
	text-align: center;
	line-height: 20px;
	margin: 0px 5px 0px 5px;
}
.viewarea .view-panel .detail-info .de-info-txt .txt-one{
	border: 1px solid #F0A936;
	background-color: #F0A936;
	margin-left:5px;
}
.viewarea .view-panel .detail-info .de-info-txt .txt-two{
	border: 1px solid #22C260;
	background-color: #22C260;
}
.viewarea .view-panel .detail-info .de-info-txt .txt-three{
	border: 1px solid #26A1DA;
	background-color: #26A1DA;
}
.viewarea .view-panel .detail-info .de-info-txt .txt-four{
	border: 1px solid #FC3838;
	background-color: #FC3838;
}
.viewarea .view-panel .detail-info .de-info-txt .txt-five{
	border: 1px solid #CF4FC7;
	background-color: #CF4FC7;
}
.viewarea .view-panel .detail-info .de-info-txt .txt-six{
	border: 1px solid #01D3BC;
	background-color: #01D3BC;
}

.viewarea .view-panel .temp-panel{
	flex: 1;
	display:flex;
	flex-direction:column
}
.viewarea .view-panel .temp-panel .temp-basic{
	flex: 1;
}

.viewarea .view-panel .temp-detail {
	flex: 2;
	display: flex;
	flex-direction:row;
}
.viewarea .view-panel .temp-detail .temp-de-item {
	flex: 1;
	padding-left:30px;
}
.viewarea .view-panel .temp-detail .temp-de-item p{
	float: left;
	text-align: center;

}
.viewarea .view-panel .temp-detail .temp-de-item .temp-img-one img{
	width:50px;
	height:70px
}
.viewarea .view-panel .temp-detail .temp-de-item .temp-img-two img{
	width:40px;
	height:70px
}
.viewarea .view-panel .temp-detail .temp-de-item .temp-txt{
	background: url("../../images/a-wd-sd-bg.png") no-repeat center;
	background-size: cover;
	margin-top: 10px;
	margin-left:10px;
}
.viewarea .view-panel .temp-detail .temp-de-item .temp-txt.wd{
	padding: 0px 4px 0px 4px;
}
.viewarea .view-panel .temp-detail .temp-de-item .temp-txt.sd{
	padding: 0px 4px 0px 4px;
}
.maparea {
	flex:2;
	display:flex;
	flex-direction:column;
	/*add by lijw 2018-08-27 添加相对定位，定位右下角信息栏*/
	position: relative;
	background-color:rgba(0,0,0,0.2);
}
/* 热力图 提示*/
.map-maparea {
	font-size: 15px;
	position: absolute;
	z-index: 99;
	bottom: 2px;
	right: 2px;
	font-size: small;
	color: rgb(255, 250, 250);
	background-color:#00000030;
	width:75px;
	display: none;
}
.map-maparea-colorcolumn{
	width: 10px;
	height: 90px;
	background: linear-gradient(rgb(252, 4, 4), rgb(217, 255, 0), rgb(26, 255, 5));
	margin: 5px 10px;
}
.map-sub {
	flex:1;
	height: 40px;
}
.map-panel {
	flex: 11;
}

iframe{
	border: 0px;
}


.grade-box {
	padding: 3px;
	text-align: center;
}
.grade-box p:nth-child(2){
	cursor: pointer;
}
.grade-panel {
	flex: 1;
	display: flex;
	flex-direction: column;
}
.grade-sub {
	flex: 3;
}
.grade-collection {
	flex: 1;
	display: flex;
	flex-direction: row;
}

.grade {
	font-size: 12px;
	margin-top: 10px;
	border-radius: 5px;
	padding: 2px;
	color: #000000;
	text-align: center;
}

.grade.one {
	background-color: #F61F6C;
}

.grade.two {
	background-color: #FEB802;
}

.grade.three {
	background-color: #00FF01;
}

.grade.four {
	background-color: #E7DB4F;
}

.airQualityChange {
	position: absolute;
}

.group-btn{
	height: 250px;
	width:150px;
	display: none;
}

.grid table {
	font-size: 12px;
}

.btns {
	width: 150px;
	padding-top: 30px;
	padding-left: 30px;
}
.btns p {
	width: 90px;
	height:35px;
	background: url("../../images/btn-bg.png") no-repeat center;
	background-size:100% 100%;
	color: #ffffff;
	line-height: 35px;
	padding-left: 15px;
	margin-top: 15px;
	cursor: pointer;
}
.btns p.active {
	background: url("../../images/btn-active-bg.png") no-repeat center;
	background-size:100% 100%;
}
.dataList {
	height:200px;
	width:300px;
	display: none;
}

.dataList .title {
	color: #ffffff;
	text-align: center;
	line-height:20px;
	margin-top:15px;
}
.notes
{
	color: #ffffff;
	font-size:12px;
	padding:5px 5px 5px 5px;
	margin-top: 50%;
}

.legend-panel{
	margin:20px 10px 0px 10px;
	padding-top: 10px;
	padding-bottom: 10px;
	background-color:#043857;
}
.legend-title {
	color: #ffffff;
	text-align: center;
	font-size: 12px;
}
.legend-sub {
	display: flex;
	flex-direction: column;
	width:70%;
	margin: 0px auto;
	margin-top: 10px;
}
.legend-item {
	flex: 1;
	display: flex;
	flex-direction: row;
	font-size:12px;
	color:#ffffff;
	padding-top: 5px;
}
.legend-item p{
	flex: 1;height:20px;
}
.legend-item p.one{
	background-color: #07FE30;
}
.legend-item p.two{
	background-color: #F6D41A;
}
.legend-item p.three{
	background-color: #E65711;
}
.legend-item p.four{
	background-color: #F11010;
}


@media only screen and (max-width: 1024px) {
.viewarea {
	flex: 0 0 250px; 
}
.remind {
	font-size:10px;
}
.viewarea .view-panel  .label {
	color: #089CA9;
	font-size: 11px;/*需要根据分辨率调*/
	font-weight: bold;
	line-height:18px;
	padding-left: 10px;
}
.viewarea .view-panel .basic-info {
	font-size:14px;
}
.viewarea .view-panel .detail-info {

	padding-top:10px;
}

.viewarea .view-panel .detail-info .de-info-num{
	flex:2;
	
}
.viewarea .view-panel .detail-info .de-info-num p{
	line-height: 40px;
	font-size:12px;
	
}
.viewarea .view-panel .detail-info .de-info-item p.number{
	line-height: 30px;
	
}
.viewarea .view-panel .detail-info .de-info-num .num-bg-one {
	background-size: 100% 100%;
}
.viewarea .view-panel .detail-info .de-info-num .num-bg-two {
	background-size: 100% 100%;
}
.viewarea .view-panel .detail-info .de-info-num .num-bg-three {
	background-size: 100% 100%;
}
.viewarea .view-panel .detail-info .de-info-num .num-bg-four {
	background-size: 100% 100%;
}
.viewarea .view-panel .detail-info .de-info-num .num-bg-five {
	background-size: 100% 100%;
}
.viewarea .view-panel .detail-info .de-info-num .num-bg-six {
	background-size: 100% 100%;
}
.viewarea .view-panel .detail-info .de-info-txt{
	flex:2;
	margin-top:5px;

}
.viewarea .view-panel .detail-info .de-info-txt .txt{
	flex:1;
	width:29px;
	font-size:10px;
	margin: 0px 0px 0px 4px;
}
.viewarea .view-panel .temp-detail .temp-de-item {
	padding-left:20px;
}

.viewarea .view-panel .temp-detail .temp-de-item .temp-img-one img{
	width:40px;
	height:60px
}
.viewarea .view-panel .temp-detail .temp-de-item .temp-img-two img{
	width:30px;
	height:60px;
}
.viewarea .view-panel .temp-detail .temp-de-item .temp-txt{
	font-size:14px;
}
.viewarea .view-panel.b {
	background-size:100% 100%;
}
.viewarea .view-panel.rb {
	background-size:100% 100%;
}
.viewarea .view-panel.lb {
	background-size:100% 100%;
}
.grade-box {
	padding: 3px;
	text-align: center;
}
.grade-collection {
	width: 250px;
}
.grade-box {
	font-size: 12px;
	padding: 0px!important;

}
.grade-box .date {
	padding-left: 4px;
	font-size: 10px;
}
.grade {
	/*font-size: 10px;*/
	width: 20px;
	margin-top: 10px;
	border-radius: 5px;
	padding: 2px;
	color: #000000;
	background-color: #ff0000;
}
.dataList {
	width: 180px;
}
.dataList .title {
	font-size: 12px;
}

}
