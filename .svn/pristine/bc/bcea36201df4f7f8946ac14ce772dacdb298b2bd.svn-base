* {
	margin: 0px;
	padding: 0px;
}
a {
	text-decoration: none;
}
iframe {
	border: none;
}
.nav-pane {
	margin-left: 0px;
	overflow-y:auto;
	width:350px;
}
.query-pane {
	font-size: 14px;
}
.query-pane .field label {
	font-size: 14px;
}
.query-pane .op button {
	padding: 0px;
	padding-right: 5px;
	color: white;
	background-color: rgba(2, 162, 233, 0.6);
	font-size: 14px;
}
.grid th, .grid td {
	font-size: 14px;
}
.selBox {
	width:100%;margin-bottom:10px;
}
select {background-color:#012235;
	border: 1px solid #1E4769;
	border-radius:5px;
	text-align: center;
	line-height:25px;
	color:#ffffff;
	width: 69%;
	padding-top:5px;
	padding-bottom:5px;
}
select::-webkit-inner-spin-button { visibility: hidden; }
select::-webkit-calendar-picker-indicator {
	background-color: #012235;
	color: #0FA2D3;
	font-size: 18px;
}

#hintInfoFrame {
	margin: 0px;
	background-color: #081327ad;
	/*background-color: red;*/
	font-size: 15px;
	width: 200px;
	height: 125px;
	position: absolute;
	z-index: 99;
	bottom: 2px;
	right: 2px;
	font-size: small;
	color: #fff;
}
#hintInfoFrame p{
	margin: 0px;
	padding-top: 10px;
	padding-left: 10px;
	padding-bottom: 10px;
}
#hintInfoFrame table{
	padding-left: 18px;
}
.workarea{
	position: relative;
	overflow: hidden;
}
.workarea iframe{
	border: 0px;
}

.grid td{
	cursor: pointer;
}
.grid td:hover{
	background-color:#155b9c;
}

@media only screen and (max-width: 1024px) {
	.nav-pane {
		width:320px;
	}
}