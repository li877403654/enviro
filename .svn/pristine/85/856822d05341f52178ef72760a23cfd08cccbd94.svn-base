a {
text-decoration: none;
}
iframe {
border: none;
}
.nav-pane {
margin-left: 0px;
overflow-y:auto;
width:350px;
}
.query-pane .op button {
padding: 0px;
padding-right: 5px;
color: white;
background-color: rgba(2, 162, 233, 0.6);
font-size: 14px;
}
.grid th, .grid td {
font-size: 14px;
}

td.getMap:hover{
background-color:#155b9c;
cursor: pointer;
}
.workarea{
overflow: hidden;
}

#hintInfoFrame {
	background-color: #081327ad;
	/*background-color: red;*/
	font-size: 15px;
	width: 230px;
	position: absolute;
	z-index: 99;
	bottom: 2px;
	right: 2px;
	font-size: small;
	color: #fff;
}
#hintInfoFrame p{
	padding-top: 10px;
	padding-left: 10px;
	margin: 0px;
}
#hintInfoFrame p:nth-child(2){
	padding-top: 0;
	padding-bottom: 10px;
}
#hintInfoFrame table{
	padding-left: 18px;
}
@media only screen and (max-width: 1024px) {
	.nav-pane {
		width:320px;
	}
}
