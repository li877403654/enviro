.head-con{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  height: 4vw;
  text-align: center;
  line-height: 4vw;
  font-size: 2.5vw;
}
.main-contain{
  position: fixed;
  top: 4vw;
  left: 1vw;
  right: 1vw;
  bottom: 1vw;
}
.left-part, .right-part, .mid-part{
  width: 0;
}
.left-box-1,.left-box-2,.left-box-3,.right-box-1,.right-box-2{
  height: 0;
}
.box-con{
  padding: 1vw;
  border: 1px solid #ddd;
  background: #f8f8f8;
}
.box-title{
  font-size: 1.2vw;
}
.left-box-2{
  margin: 1vw 0;
}
.mid-part{
  margin: 0 1vw;
  border:solid #00B83F;
}
.right-box-2 {
  margin-top: 1vw;
}