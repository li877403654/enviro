html,
body {
	width: 100%;
	height: 100%;
	margin: 0;
	padding: 0;
}
body {
	background: url('../../images/test1/bg.jpg') no-repeat;
	background-size: 100% 100%;
	display: flex;
	flex-direction: column;
	font-family: "Microsoft Yahei";
}
iframe {
	border: none;
}

.header {
	display: flex;
	flex-direction: row;
	height: 10%;
	max-height: 100px;
	min-height: 50px;
}
.header .left-bar,
.header .right-bar {
	flex: 1;
	min-width: 280px;
	background-repeat: no-repeat;
	background-size: 100% 100%;
	box-sizing: border-box;
	padding-top: 18px;
	position: relative;
}
.header .left-bar .info,
.header .right-bar .info {
	position: absolute;
	top: 19%;
	height: 30%;
	width: 100%;
	color: #41eaff;
	font-size: 16px;
	line-height: 1em;
	display: flex;
	flex-direction: row;
	align-items: center;
}
.header .left-bar .info .item,
.header .right-bar .info .item {
	height: 100%;
	display: flex;
	align-items: center;
	margin-right: 0.8vw;
}
.header .left-bar .info .item img,
.header .right-bar .info .item img {
	vertical-align: middle;
	display: inline-block;
	margin-right: 0.6vw;
}
.header .left-bar .info .item span,
.header .right-bar .info .item span {
	display: block;
}
.header .left-bar {
	background-image: url('../../images/test1/header-left-bar-bg.png');
}
.header .left-bar .info {
	left: 1.8vw;
}
.header .right-bar {
	background-image: url('../../images/test1/header-right-bar-bg.png');
	text-align: right;
}
.header .right-bar .info {
	right: 1.8vw;
}
.header .right-bar .info .space {
	flex: 1;
}
.header .right-bar .info a {
	display: block;
	text-decoration: none;
	color: inherit;
}
.header .logo {
	flex: 1;
	margin-left: -40px;
	margin-right: -40px;
	margin-top: -1px;
	display: flex;
	justify-content: center;
	align-items: center;
	position: relative;
	overflow: visible;
	height: 100%;
}
.header .logo img {
	max-width: 100%;
	max-height: 100%;
	min-width: 100px;
}
.header .logo::after {
	content: '';
	display: block;
	position: absolute;
	left: 40px;
	right: 40px;
	top: 100%;
	border-bottom: solid 1px rgba(26, 111, 166, 0.88);
}
.tab-box {
	flex: 1;
	display: flex;
	flex-direction: column;
	background: url('../../images/test1/box-bg.png') no-repeat;
	background-size: 100% 100%;
	padding-left: 1.4583%;
	padding-right: 1.4583%;
	padding-top: 2px;
}
.tab-bar {
	padding-left: 2%;
	padding-right: 2%;
	margin-bottom: 1.31vh;
	position: relative;
}
.tab-bar::before,
.tab-bar::after {
	content: '';
	display: block;
	position: absolute;
	width: 31%;
	top: 33.333%;
	height: 66.666%;
	border-top: solid 1px #03425c;
}
.tab-bar::before {
	left: 0;
	border-left: solid 1px #03425c;

}
.tab-bar::after {
	right: 0;
	border-right: solid 1px #03425c;
}
.tab-bar ul {
	list-style: none;
	margin: 0;
	padding: 0;
	display: flex;
	width: 40%;
	margin: 0px auto
}
.tab-bar li {
	flex: 1;
}
.tab-bar a {
	display: block;
	text-decoration: none;
	color: #49d9fe;
	text-align: center;
	font-size: 22px;
	line-height: 58px;
	background: url('../../images/test1/tab-bg.png') no-repeat;
	background-size: 100% 100%;
}
.tab-bar li.selected a {
	color: white;
	background-image: url('../../images/test1/tab-selected-bg.png');
}
.panes-container {
	flex: 1;
	display: flex;
	flex-direction: row;
	margin-bottom: 3.888vh;
}

.map-nav {
	height: 60px;
	width: 50%;
	position: absolute;
	top: 7%;
	left: 25%;
}
.item-nav {
	padding-left: 20%;
	color: #ffffff;
	height: 40px;
}
.item-nav > li {
	width: 18%;
	height: 40px;
	line-height: 40px;
	text-align: center;
	float: left;
	background: url("../../images/item-nav-bg.png") no-repeat left top;
	background-size: 100% 100%;
	cursor: pointer;
	list-style: none;
}
.item-nav > li.margin{
	margin-left: 20px;
	margin-right: 20px;
}
.item-nav-active {
	background: url("../../images/item-nav-active.png") no-repeat left top !important;
	background-size: 100% 100% !important;
}

