.main-pane {
	flex: 1;
	border: solid 1px #02a2e9;
	margin: 0 1.36vw 0 8px;
}
.main-pane iframe {
	border: 0;
	width: 100%;
	height: 100%;
	background-color: rgba(23, 51, 115, 0.1);
}



@media only screen and (max-width: 1200px), screen and (max-height: 800px) {
	.header .left-bar .info,
	.header .right-bar .info {
		font-size: 14px;
	}
	.header .left-bar .info .item img,
	.header .right-bar .info .item img {
		width: 16px;
	}
	.header .logo {
		margin-left: -50px;
		margin-right: -50px;
	}
	.header .logo::after {
		left: 50px;
		right: 50px;
	}
	.tab-bar a {
		font-size: 16px;
		line-height: 44px;
	}
	.nav-pane {
		padding-left: 10px;
		padding-right: 10px;
	}
	.nav-pane .group-btn {
		font-size: 14px;
		line-height: 40px;
	}
	.nav-pane .group-btn::before,
	.nav-pane .group-btn::after {
		background-size: 6px 41px;
	}
	.nav-pane .group-btn.expandable i {
		width: 14px;
		height: 14px;
	}
	.nav-pane .group-items a {
		font-size: 16px;
		line-height: 32px;
	}
	.main-pane {
		margin-left: 6px;
	}
}
@media only screen and (max-height: 700px) {
	.tab-bar a {
		font-size: 14px;
		line-height: 32px;
	}
	.panes-container {
		margin-bottom: 18px;
	}
}



@media only screen and (max-width: 1024px) {
 .item-nav > li {
	height: 30px;
	line-height: 30px;
	font-size:12px;
 }
 
}
